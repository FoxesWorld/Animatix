{
  "animationName": "imageAnimation",
  "imagePath": "img/diamond.png",
  "repeat": true,
  "bounds": {
    "x": 100,
    "y": 100,
    "width": 350,
    "height": 350
  },
  "phases": [
    {
      "imageEffects": ["resize"],
      "name": "resize",
      "duration": 800,
      "effectParams": {
        "resize": {
          "startWidth": 290,
          "startHeight": 290,
          "endWidth": 100,
          "endHeight": 100,
          "resizeType": "SCALE_TO_FIT"
        }
      }
    },
    {
      "imageEffects": ["crack"],
      "name": "resize",
      "duration": 1200,
      "effectParams": {
        "crack": {
          "numCracks": 30
        }
      }
    },
    {
      "imageEffects": ["decay", "move"],
      "name": "move to",
      "duration": 1000,
      "effectParams": {
        "move": {
          "startX": 100,
          "startY": 100,
          "endX": 190,
          "endY": 50,
          "traceColor": "gray",
          "traceThickness": 2
        },
        "rotate": {
          "startAngle": 0,
          "endAngle": 180
        }
      }
    }

  ]
}
