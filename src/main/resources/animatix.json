{
  "animationName": "imageAnimation",
  "repeat": false,
  "phases": [
    {
      "imageEffects": ["resize"],
      "name": "resize",
      "duration": 800,
      "effectParams": {
        "resize": {
          "startWidth": 190,
          "startHeight": 190,
          "endWidth": 90,
          "endHeight": 90,
          "resizeType": "SCALE_TO_COVER"
        }
      }
    },
    {
      "imageEffects": ["move"],
      "name": "move to",
      "duration": 1000,
      "effectParams": {
        "move": {
          "startX": 50,
          "startY": 50,
          "endX": 190,
          "endY": 50
        }
      }
    },
    {
      "imageEffects": ["COLOR_FADE"],
      "name": "color fade",
      "duration": 500,
      "effectParams": {
        "colorFade": {
          "startAlpha": 1.0,
          "endAlpha": 0.0
        }
      }
    },
    {
      "imageEffects": ["decay"],
      "name": "decay effect",
      "duration": 2000,
      "effectParams": {
        "decay": {
          "startAlpha": 1.0,
          "endAlpha": 0.0,
          "decaySpeed": 10
        }
      }
    }
  ],
  "imagePath": "fox.png"
}
